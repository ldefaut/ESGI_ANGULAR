{
  "hosting": [
    {
      "target": "kanban-fire",
      "public": "./dist/kanban-fire",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "rewrites": [
        {
          "source": "/fr/**",
          "destination": "/fr/index.html"
        },
        {
          "source": "/en-US/**",
          "destination": "/en-US/index.html"
        }
      ],
      "i18n": {
        "root": "/"
      }
    }
  ]
}
